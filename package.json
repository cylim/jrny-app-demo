{
  "name": "jrny-app-demo",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "bunx convex dev --once && concurrently -r bun:dev:web bun:dev:convex",
    "dev:web": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "bunx convex dev",
    "build": "vite build && cp instrument.server.mjs .output/server && tsc --noEmit",
    "start": "node --import ./.output/server/instrument.server.mjs .output/server/index.mjs",
    "preview": "wrangler dev",
    "deploy": "bun run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@convex-dev/better-auth": "^0.9.7",
    "@convex-dev/react-query": "^0.0.0-alpha.11",
    "@sentry/tanstackstart-react": "^10.25.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.132.2",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.132.2",
    "better-auth": "1.3.34",
    "convex": "^1.29.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.14.2",
    "@convex-dev/eslint-plugin": "^1.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/eslint-config": "^0.3.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "~9.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.48.0"
  }
}
