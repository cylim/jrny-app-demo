{
  "name": "jrny-app-demo",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "bunx convex dev --once && concurrently -r bun:dev:web bun:dev:convex",
    "dev:web": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "bunx convex dev",
    "build": "vite build && cp instrument.server.mjs dist/server && tsc --noEmit",
    "start": "node --import ./dist/server/instrument.server.mjs dist/server/index.mjs",
    "preview": "bun run build && wrangler dev --config dist/server/wrangler.json",
    "deploy": "bun run build && wrangler deploy --config dist/server/wrangler.json",
    "cf-typegen": "wrangler types",
    "seed:db": "node --env-file=.env.local scripts/seed-database.mjs",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "tsc && biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@convex-dev/better-auth": "^0.9.7",
    "@convex-dev/react-query": "^0.0.0-alpha.11",
    "@faker-js/faker": "^10.1.0",
    "@mendable/firecrawl-js": "^4.5.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/tanstackstart-react": "^10.25.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.132.2",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.132.2",
    "@useautumn/convex": "^0.0.15",
    "atmn": "^0.0.28",
    "autumn-js": "^0.1.47",
    "better-auth": "1.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.29.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.553.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@cloudflare/vite-plugin": "^1.14.2",
    "@edge-runtime/vm": "^5.0.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/ui": "^4.0.9",
    "concurrently": "~9.2.1",
    "convex-test": "^0.0.38",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.9",
    "wrangler": "^4.48.0"
  }
}
